<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NumberPicker">
        <attr name="lsmDefaultValue" format="integer" />
        <attr name="lsmMinValue" format="integer" />
        <attr name="lsmMaxValue" format="integer" />
        <attr name="lsmTextSize" format="dimension" />
        <attr name="lsmTextColor" format="color" />
        <attr name="lsmTextStyle">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
            <flag name="bold_italic" value="3" />
        </attr>
        <attr name="lsmFontname" format="string" />
        <attr name="lsmSeparatorColor" format="color" />
        <attr name="lsmEditable" format="boolean" />
        <attr name="lsmWrapped" format="boolean" />
    </declare-styleable>

</resources>